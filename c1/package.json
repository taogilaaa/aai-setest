    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "@warungpintar/warpin-scripts": "^0.3.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "node-nats-streaming": "^0.2.6",
    "uuid": "^3.3.3"
  }
